{
  "compilerOptions": {
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "lib": ["es6", "dom"],
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "downlevelIteration": true
  }
}


